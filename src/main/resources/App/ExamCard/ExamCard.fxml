<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="newQuizPane" alignment="CENTER" minHeight="220.0" minWidth="200.0" prefHeight="220.0" prefWidth="290.0" spacing="20.0" style="-fx-background-color: #000;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.ExamCard.ExamCardController">
   <children>
      <Label fx:id="ExamCardLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="284.0" text="Add Exam" textAlignment="CENTER" textFill="#f3eae0">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <ImageView fitHeight="80.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/ExamIcon2.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Button fx:id="ExamCardButton" mnemonicParsing="false" prefHeight="42.0" prefWidth="148.0" style="-fx-background-color: #f3eae0;" text="Add" textFill="#080303">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</VBox>
